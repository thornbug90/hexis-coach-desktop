{
  "name": "Hexis Coach Desktop Web App",
  "dockerComposeFile": ["docker-compose.yaml"],
  "service": "coach-desktop",
  "workspaceFolder": "/workspaces/coach-desktop",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "GraphQL.vscode-graphql-syntax",
        "redhat.vscode-yaml",
        "streetsidesoftware.code-spell-checker",
        "yoavbls.pretty-ts-errors"
      ]
    }
  },
  "forwardPorts": [3000],
  "postCreateCommand": "npm ci --legacy-peer-deps && cp .env.example .env"
}
